new basket,
  masterEntryCh,
  applicationCh,
  resultCh,
  stdout(`rho:io:stdout`),
  deployerId(`rho:rchain:deployerId`),
  registryLookup(`rho:registry:lookup`)
in {

  stdout!("APPLICATION_ID") |
  registryLookup!(`rho:id:MULTISIG_REGISTRY_URI`, *masterEntryCh) |

  for (masterEntry <= masterEntryCh) {
    stdout!("PUBLIC_APPLY") |
    masterEntry!(("PUBLIC_APPLY", "APPLICATION_ID", bundle+{*applicationCh}, bundle+{*resultCh})) |

    // application accepted
    for (@key <- applicationCh) {
      stdout!("application was accepted") |
      @(*deployerId, "rchain-multisig", "MULTISIG_REGISTRY_URI")!(key)
    } |

    // application recorded
    for (@r <- resultCh) {
      match r {
        String => {
          basket!({ "status": "failed", "message": r }) |
          stdout!(("failed", r))
        }
        (true, p) => {
          basket!({ "status": "completed", "message": p }) |
          stdout!(p)
        }
      }
    }
  }
}
